<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.mid.ec.api.service.reservedfound.ReservedFundRepository">
    
<insert id="insertReservedFundContent" parameterType="Map">
	insert into reservedfund_luoyang(id,content,insert_date,idno,buscode,percode,corpcode,fetchrea,fetchtype
		,curcode,curname,curdepcode,isdelacc,auditflag,relno,allrequiremny,allstandmny,houseno,isAuto
		,source,bkcard,yhhb,bkcardname,skzhmc)
	values(#{id},#{content},now(),#{idno},#{buscode},#{percode},#{corpcode},#{fetchrea},#{fetchtype}
		,#{curcode},#{curname},#{curdepcode},#{isdelacc},#{auditflag},#{relno},#{allrequiremny}
		,#{allstandmny},#{houseno},#{isAuto},#{source},#{bkcard},#{yhhb},#{bkcardname},#{skzhmc})

</insert>

    </mapper>